<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.niuma.questionnaire.dao.AnswerMapper">
    <resultMap id="BaseList" type="com.niuma.questionnaire.entity.Answer">
        <result property="id" column="ID"/>
        <result property="content" column="content"/>
    </resultMap>
    <select id="getAnswerByTest" resultType="com.niuma.questionnaire.entity.Answer">
        select *
        from questionnaire.answer;
    </select>
</mapper>
